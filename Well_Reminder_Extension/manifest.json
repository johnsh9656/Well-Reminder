{
    "manifest_version": 3,
    "name": "Well Reminder",
    "description": "Provides reminders and help-lines to support mental health.",
    "version": "1.0",
    "permissions": [
        "tabs",
        "storage"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "logo_16.png",
        "48": "logo_48.png",
        "128": "logo_128.png"
    }
}